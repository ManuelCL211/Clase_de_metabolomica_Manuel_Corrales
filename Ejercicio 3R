nstall.packages("pacman") #Es un paquete que llama a otros paquetes y si no están, lo instala

library ("pacman") #esta función llama al paquete instalado

p_load("ggplot2", #para gráficas
       "dplyr", #para facilitar el manejo de datos
       "vroom") #llamar repositorios

Datos_curva <- vroom(file="https://raw.githubusercontent.com/ManuelLaraMVZ/resultados_PCR_practica/refs/heads/main/Amplif_grupo1_17022025.csv")
head(Datos_curva) #Solo para que me de los primeros seis datos

#A mí me tocó hacer F1

Curva_1 <- Datos_curva %>% #El nuevo objeto va a tener una asignación a partir de la base de datos XX, y le hace lo siguiente:
  select("Cycle", "F1")
Curva_1

Grafica_curva <- ggplot(Curva_1,
                        mapping=aes(x=Cycle,
                                    y=F1))+
  geom_point()+
  geom_line(color="red",
            size=1.2)

Grafica_curva

Grafica_curva_2<- Grafica_curva+
  labs(x="ciclos",
       y="fluorescencia",
       title="Creado por: Manuel Corrales",
       subtitle="Gráfica F1",
       caption="Ejercicio 3")+
  theme_classic()

Grafica_curva_2

